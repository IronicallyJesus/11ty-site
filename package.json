{
  "name": "eleventy-docker-debug",
  "version": "1.0.0",
  "description": "An Eleventy project with Docker and VS Code debugging.",
  "scripts": {
    "prestart": "npm run build:css",
    "start": "npm-run-all -p serve:eleventy watch:css dev:api",
    "serve:eleventy": "eleventy --serve",
    "watch:css": "tailwindcss -i ./src/css/input.css -o ./_site/css/style.css --watch",
    "dev:api": "node --watch server.js",
    "build": "npm run build:css && eleventy",
    "build:css": "tailwindcss -i ./src/css/input.css -o ./_site/css/style.css --minify",
    "test": "playwright test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.58.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "async-mutex": "^0.5.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "fs-extra": "^11.3.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@tailwindcss/postcss": "^4.1.12",
    "autoprefixer": "^10.4.19",
    "http-proxy-middleware": "^3.0.0",
    "luxon": "^3.7.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.17"
  }
}